@model ThriftManager.DTO.Requests.AddMemberRequest
<link href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" rel="stylesheet" />

<form asp-action="AddContributingMember" method="post" class="p-4 border rounded shadow-sm bg-light">
    <div class="form-group">
        <label asp-for="MemberId" class="form-label"></label>
        <input asp-for="MemberId" class="form-control" />
    </div>
    <button type="submit" class="btn btn-primary">Add Member</button>
</form>
